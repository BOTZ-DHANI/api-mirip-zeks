  "bugs": {
    "url": "https://github.com/keymetrics/km.js/issues"
  },
  "homepage": "https://github.com/keymetrics/km.js#readme",
  "dependencies": {
    "async": "^2.6.3",
    "axios": "^0.21.0",
    "debug": "~4.3.1",
    "eventemitter2": "^6.3.1",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babelify": "10.0.0",
    "browserify": "^17.0.0",
    "jsdoc": "^3.4.2",
    "minami": "^1.1.1",
    "mocha": "^7.0.0",
    "should": "*",
    "uglify-es": "~3.3.9"
  },
  "browserify": {
    "debug": true,
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "debug": false,
                "forceAllTransforms": true
              }
            ]
          ],
          "sourceMaps": true
        }
      ]
    ]
  },
  "browser": {
    "./src/auth_strategies/embed_strategy.js": false,
    "ws": false
